{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest/globals": true,
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off"
  },
  "plugins": ["jest"],
  "extends": "google"
}
